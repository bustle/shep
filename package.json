{
  "name": "shep",
  "version": "1.0.0-beta.1",
  "description": "A framework for building APIs with AWS API Gateway and Lambda",
  "main": "lib/index.js",
  "scripts": {
    "compile": "./node_modules/.bin/babel -d lib/ src/",
    "test": "ava",
    "lint": "jshint --exclude ./node_modules .",
    "prepublish": "npm run compile"
  },
  "keywords": [
    "serverless",
    "serverless framework",
    "api gateway",
    "lambda",
    "aws",
    "aws lambda",
    "amazon",
    "amazon web services",
    "shepherd",
    "shep",
    "lambda framework"
  ],
  "author": "Steve Faulkner <steve@bustle.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/bustlelabs/shep"
  },
  "bin": {
    "shep": "cli.js"
  },
  "dependencies": {
    "archiver": "^1.0.0",
    "aws-sdk": "^2.4.13",
    "bluebird": "^3.4.0",
    "chalk": "^1.1.3",
    "fs-extra-promise": "^0.3.1",
    "glob": "^7.0.0",
    "globby": "^5.0.0",
    "inquirer": "^1.0.3",
    "lodash": "^4.7.0",
    "loud-rejection": "^1.6.0",
    "observatory": "^1.0.0",
    "stream-buffers": "^3.0.0",
    "webpack": "^2.1.0-beta.20",
    "yargs": "^4.8.1"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.5.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.7.2",
    "testdouble": "^1.4.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports",
      "transform-object-rest-spread"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  }
}
